#include<stdio.h>
    #include<math.h>
    int main(){
        int i,n,count=0;
        scanf("%d",&n);
        for(i=1;i<n;i++)
        {
            if(n%i==0)
            count=count+1;
        }
        if(count==n)
        {
            printf("Perfect number");
        }
        else
        {
            printf("Not a Perfect number");
        }
        return 0;
    }
