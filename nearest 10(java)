import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int rem=n%10;
        int a=(n+(10-rem));
        int b=(n-rem);
        if(rem>=5)
        {
            System.out.println(a);
        }
        else
        {
            System.out.println(b);
        }
    }
}
