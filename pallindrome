 #include<stdio.h>
 #include<math.h>
 int main(){
     int n,d,r,temp;
     scanf("%d",&n);
     temp=n;
     while(n>0)
     {
         d=n%10;
         n=n/10;
         r=r*10+d;
         }
         if(r==temp)
         {
             printf("Pallindrome");
         }
         else
         {
             printf("Not a Pallindrome");
         }
 }
