import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        int n;
        Scanner s=new Scanner(System.in);
        n=s.nextInt();
        int k=countDigits(n);
        System.out.println(k);
    }
    public static int countDigits(int n)
    {
        int d=(int)Math.log10(n)+1;
        int f;
        f=n%d;
        return f;
    }
}
